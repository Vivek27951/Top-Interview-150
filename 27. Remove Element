class Solution {
public:
    int removeElement(vector<int>& nums, int val) {
        int last = nums.size() - 1;
        int curr = 0;
        while(curr<=last){
            if(nums[curr]==val){
                swap(nums[curr],nums[last]);
                last--;
            } else {
                curr++;
            }
        }
        return curr;
    }
};
