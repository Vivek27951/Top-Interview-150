class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        int k = 0;
        int i = 1;
        while(i<nums.size()){
            if(nums[k]==nums[i]){
                i++;
            } else {
                nums[k+1] = nums[i];
                i++; k++;
            }
        }
        return k+1;
    }
};
